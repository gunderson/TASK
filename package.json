{
  "name": "TASK",
  "version": "1.0.0",
  "description": "Tool Application Starter Kit",
  "main": "dist/main.js",
  "scripts": {
    "addPage": "node ./custom_node_modules/addPage",
    "start": "gulp --env dev && node --harmony ./dist/main --env dev",
    "watch": "gulp --env dev && (gulp watch --env dev &) && node --harmony ./dist/main --env dev"
  },
  "engines": {
    "node": "^6.0.0"
  },
  "browserify": {
    "debug": true,
    "externalHelpers": false,
    "runtimeHelpers": true,
    "transform": [
      [
        "babelify",
        {
          "compact": false,
          "presets": [
            "es2015"
          ],
          "extensions": [
            ".js"
          ]
        }
      ]
    ]
  },
  "stringify": {
    "appliesTo": {
      "includeExtensions": [
        ".txt",
        ".vert",
        ".frag"
      ]
    }
  },
  "aliasify": {
    "aliases": {
      "underscore": "lodash"
    },
    "replacements": {
      "_TASK/(\\w+)": "./src/TASK/js/$1",
      "_art-kit/(\\w+)": "/Users/pg/Development/repositories/art-kit/src/$1",
      "_gsap/(\\w+)": "./node_modules/gsap/src/uncompressed/$1",
      "postprocessing": "/Users/pg/Development/repositories/postprocessing/src/index.js"
    }
  },
  "browser": {
    "jquery.gsap": "./node_modules/gsap/src/minified/jquery.gsap.min.js"
  },
  "author": "Patrik Gunderson",
  "license": "MIT",
  "repository": "https://github.com/gunderson/TASK.git",
  "dependencies": {
    "acorn": "^3.2.0",
    "art-kit": "^2.0.0",
    "audio-analyser": "^1.0.2",
    "backbone-events-standalone": "^0.2.7",
    "body-parser": "^1.15.0",
    "domain": "0.0.1",
    "express": "^4.13.4",
    "forever": "^0.15.1",
    "gsap": "^1.18.4",
    "howler": "^1.1.29",
    "jquery": "^2.2.3",
    "jquery.browser": "^0.1.0",
    "lodash": "^4.11.2",
    "marked": "^0.3.5",
    "method-override": "^2.3.5",
    "mic": "^2.1.1",
    "multer": "^1.1.0",
    "npm": "^3.8.8",
    "page": "^1.7.1",
    "q": "^1.4.1",
    "serve-favicon": "^2.3.0",
    "socket.io": "^1.4.8",
    "three": "*"
  },
  "autoupdate": {
    "pollFrequency": 60000,
    "srcPath": "/usr/src/app/",
    "remoteHost": "raw.githubusercontent.com",
    "remotePath": "/gunderson/ditkajs-client/master/package.json"
  },
  "build-id": "26fe926c40621bc46667855744ed1fffb91c0841388f6424a73736c982dcb239",
